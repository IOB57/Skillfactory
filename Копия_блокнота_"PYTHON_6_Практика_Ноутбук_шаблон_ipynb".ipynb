        "\n",
        "input_string = input('Введите последовательность чисел: ')\n",
        "# ваш код здесь\n",
        "def find_median(first_half, second_half, numbers):\n",
        "    # Если список имеет чётную длину\n",
        "    if len(numbers) % 2 == 0:\n",
        "        result = (second_half[0] + first_half[-1]) / 2\n",
        "    else:\n",
        "        # Если длина нечётная, медианой является центральный элемент\n",
        "        result = numbers[len(numbers) // 2]\n",
        "    return result\n",
        "\n",
        "\n",
        "numbers = list(map(int, input_string.split(', ')))\n",
        "\n",
        "# Сортируем список\n",
        "numbers.sort()\n",
        "print(numbers)\n",
        "x = 0\n",
        "for range in numbers:\n",
        "  x = x + 1\n",
        "# Проверка четности не четности списка\n",
        "if x // 2:\n",
        "   mid = len(numbers) // 2\n",
        "   first_half = numbers[:mid]\n",
        "   second_half = numbers[mid:]\n",
        "   find_median (first_half, second_half, numbers)\n",
        "else:\n",
        "   first_half = None\n",
        "   second_half = None\n",
        "   find_median (first_half, second_half, numbers)\n",
        "\n",
        "median = find_median(first_half, second_half, numbers)\n",
