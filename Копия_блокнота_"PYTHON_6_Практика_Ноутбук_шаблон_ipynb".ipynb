        "def find_min_max(user_input):\n",
        "    elements = user_input.split()\n",
        "\n",
        "    numbers = []\n",
        "    for el in elements:\n",
        "        try:\n",
        "            numbers.append(float(el.replace(',', '.')))\n",
        "        except ValueError:\n",
        "            continue\n",
        "\n",
        "    # Находим минимум и максимум\n",
        "    minimum = min(numbers)\n",
        "    maximum = max(numbers)\n",
        "\n",
        "    return minimum, maximum\n",
        "\n",
        "# Ввод данных\n",
        "user_input = input(\"Введите числа через пробел (с дробными частями через запятую): \")\n",
        "\n",
        "\n",
        "minimum, maximum = find_min_max(user_input)\n",
        "\n",
        "# Вывод результата\n",
        "print(f\"Minimum: {minimum}\")\n",
        "print(f\"Maximum: {maximum}\")"
